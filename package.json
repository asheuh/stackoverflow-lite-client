{
  "name": "Stackoverflow-lite-web",
  "version": "1.0.0",
  "description": "The frontend part of the stackoverflow-lite application using ES6",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch & live-server public/",
    "watch": "webpack --watch",
    "build": "webpack --config webpack.config.js",
